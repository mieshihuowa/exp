<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yangli.experiment.dao.StudentsDao">
    <!--配置返回数据类型-->
    <resultMap type="map" id="studentList">
        <id column="id" property="id" jdbcType="INTEGER" />
        <result column="sno" property="sno" jdbcType="INTEGER" />
        <result column="sname" property="sname" jdbcType="VARCHAR" />
      <!--  <result column="cno" property="cno" jdbcType="INTEGER" />
        <result column="passwd" property="passwd" jdbcType="VARCHAR" /> --></resultMap>
    <!--查询全部-->
    <select id="queryAll" resultMap="studentList">
        select id,sno,sname from students
    </select>

    <insert id="insert" parameterType="Students" useGeneratedKeys="true" keyProperty="id">
        insert into students (sno, sname, cno, passwd) values (#{sno},#{sname},#{cno},#{passwd})
    </insert>

</mapper>


